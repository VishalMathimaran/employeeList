<% include header.ejs %>
<div class="table-wrapper-scroll-y my-custom-scrollbar custom">
<table class="table table-bordered table-striped mb-0">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">City</th>
      <th scope="col">Email</th>
      <th scope="col">Phone no</th>
    </tr>
  </thead>
  <tbody>
    <% employee.forEach(function(emp){ %>
    <tr>
      <td><%= emp.name %></td>
      <td><%= emp.city %></td>
      <td><%= emp.email %></td>
      <td><%= emp.phoneno %></td>
    <% }) %>
  </tbody>
</table>
</div>
<p><%=employee%></p>
<% include footer.ejs %>
